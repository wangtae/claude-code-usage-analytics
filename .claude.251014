hange\n        // Banner click tracking\n        function trackBannerClick(bannerId, event) {\n            // 관리자는 배너 추적하지 않음\n            if (isAdmin) {\n                console.log('Admin user - banner tracking skipped');\n                // 링크가 있는 경우 기본 동작은 허용\n                if (!event.target.closest('a')) {\n                    event.preventDefault();\n                }\n                return;\n            }\n            \n            // AJAX로 클릭 기록\n            fetch('/go/banner_click.php', {\n                method: 'POST',\n                headers: {\n                    'Content-Type': 'application/x-www-form-urlencoded',\n                    'X-Requested-With': 'XMLHttpRequest'\n                },\n                body: 'banner_id=' + bannerId\n            })\n            .then(response => response.json())\n            .then(data => {\n                if (data.success) {\n                    console.log('Banner click tracked');\n                }\n            })\n            .catch(error => {\n                console.error('Error tracking banner click:', error);\n            });\n            \n            // 링크가 있는 경우 기본 동작 허용, 없는 경우 이벤트 차단\n            if (!event.target.closest('a')) {\n                event.preventDefault();\n            }\n        }\n        \n        // Banner rotation and scroll effect\n                \n        document.addEventListener('DOMContentLoaded', function() {\n            const deliveryRadios = document.querySelectorAll('input[name=\"delivery_type\"]:not([disabled])');\n            \n            // 배송비 표시/숨김 처리 함수\n            function toggleDeliveryFee() {\n                const deliveryFeeRow = document.getElementById('deliveryFeeRow');\n                const deliveryFeeAmount = document.getElementById('deliveryFeeAmount');\n                const selectedDelivery = document.querySelector('input[name=\"delivery_type\"]:checked');\n                \n                if (!deliveryFeeRow) return;\n                \n         