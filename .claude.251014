              "content": "<?php\n/**\n * 전화 녹취 연동 모듈 (인터넷전화 제공 기업에서 제공하는 녹취모듈을 랩핑하는 모듈)\n *\n * @param string $_GET [\"code\"] 암호문 = \"교육센터아이디|강사아이디|조회전화번호1|조회전화번호2|조회날짜\"\n * @return string 아이프레임에 출력되는 html 문서\n */\ninclude_once \"./_common.php\";\ninclude_once $xn[lib_path] . \"/common.lib.php\";\ninclude_once $xn[lib_path] . \"/cts.lib.php\";\n\n$script_path = get_script_path(__FILE__);\n\n\n\nif (isset($_GET[\"mp3\"])) { // mp3 url을 수동으로 올린경우 처리\n\n    $mp3 = $jt->decrypt(urlencode($_GET[\"mp3\"]));\n\n    if (!strstr($mp3, \"http://\")) {\n        $mp3 = \"http://\" . $mp3;\n    }\n\n    $download = $script_path . \"/download.php?file=\" . $mp3;\n\n    $audio_link_str = '<div style=\"margin-top:30px;text-align:center;width:100%;font-weight:bold;font-size:20px\"><a href=\"javascript:win_open(\\'' . $mp3 . '\\', \\'\\', \\'resizable=0,scrollbars=no,status=yes,width=300,height=200\\');\" download>음원듣기</a>';\n    $download_link_str = '<a href=\"' . $mp3 . '\" download>다운로드1</a> | <a href=\"' . $download . '\">다운로드2</a>';\n\n\n    echo $audio_link_str . \" | \" . $download_link_str;\n\n    exit;\n}\n\n\nglobal $js;\nglobal $js_idx;\n$code = $jt->web_dec(urlencode($_GET[\"code\"]));\nif ($jt->is_dev_ip()) {\n    //echo $code;\n    echo $domain_name;\n}\n$code_arr = explode(\"|\", $code);\n\n\nif ( !$member['mb_id'] ) {\n    echo '접근 권한이 없습니다.';\n    exit;\n}\n\n\n$info = array();\n\n\n/**\n * 녹취모듈별로 사용가능한 다양한 포멧으로 검색조건을 설정한다.\n */\n$info[\"교육센터아이디\"] = $code_arr[0];\n$info[\"강사아이디\"] = $code_arr[1];\n$info[\"전화번호1\"] = trim($code_arr[2]); // mb_hp\n$info[\"전화번호2\"] = trim($code_arr[3]); // mb_tel\n\nif ($jt->dev) {\n    //print_r2( $info );\n}\n\n{\n    $phone_arr = array(); // 전화번호 목록을 배열로 저장\n    $phone_arr_idx = 0;\n\n    $info[\"전화번호목록\"] = \"\";    // 전화번호 목록을 |을 구분자로 저장\n    {\n        if ($jt->ctype_phone($info[\"전화번호1\"])) {\n            $info[\"전화번호목록\"] = $info[\"전화번호1\"];\n        }\n        if ($jt->ctype_phone($info[\"전화번호2\"])) {\n            if ($info[\"전화번호목록\"]) {\n                $info[\"전화번호목록\"] .= \"|\" . $info[\"전화번호2\"];\n            } else {\n                $info[\"전화번호목록\"] = $info[\"전화번호2\"];\n            }\n        }\n    }\n}\n$info[\"수업날짜\"] = $code_arr[4]; // YYYY-MM-DD\n\n$info[\"수업시간-시\"] = $code_arr[5]; // YYYY-MM-DD\n$info[\"수업시간-분\"] = $code_arr[6]; // YYYY-MM-DD\n\n\n/* 비회원 레벨테스트 결과확인 시 로그인 상태가 아니므로 녹취열람이 불가능한 문제 수정 (2016-03-02)*/\n$info[\"비회원독립사이트\"] = $code_arr[7];\nif ($member['mb_level'] == 2) {\n    if (!$info[\"비회원독립사이트\"] and !$member['mb_id']) {\n        only_alert('접근권한이 없습니다.');\n        popup_close();\n    }\n}\n\nfunction select_telecom( $text ) {\n    global $telecom;\n    foreach ( (array)$telecom as $k => $v ) {\n        $telecom[$k] = false;\n    }\n    $telecom[$text] = true;\n}\n\n$telecom = array();\n{\n    $telecom['블루컴즈'] = false;\n    $telecom['니즈텔레콤'] = false;\n    $telecom['다이얼커뮤니케이션즈'] = false;\n}\n\nif ( $info[\"교육센터아이디\"] == \"ph_utis\" ) {\n    select_telecom('니즈텔레콤');\n}\n\nif ($info[\"교육센터아이디\"] == \"ec_emt\"  or $info[\"교육센터아이디\"] == \"ec_m4s\" or $info[\"교육센터아이디\"] == \"ec_m4s_kr\") {\n    select_telecom('블루컴즈');\n}\nif ($info[\"교육센터아이디\"] == \"ec_kwt\" or $info[\"교육센터아이디\"] == \"ec_belize\") {\n    select_telecom('다이얼커뮤니케이션즈');\n}\n\nif ($info[\"교육센터아이디\"] == \"ec_m4s\" AND strtolower($info[\"강사아이디\"]) == \"jodi\" AND $info[\"수업날짜\"] >= '2021-03-26') {\n    select_telecom('니즈텔레콤');\n}\n\nif ( $info[\"교육센터아이디\"] == \"ca_olk\" OR $info[\"교육센터아이디\"] == \"ec_engloo\"  ) { // 잉글리폰(ca_olk)\n    select_telecom('니즈텔레콤');\n}\n\nif ( $info[\"강사아이디\"] == \"jill2\" and $info[\"수업날짜\"] >= \"2023-03-06\" ) {\n    select_telecom('니즈텔레콤');\n}\n\n/**\n * 공통스크립트 추가\n */\necho '\n<style>\na {\n    text-decoration:none;\n    color:#1f9beb;\n}\na:hover {\n    color:#ff4838;\n}\n\n</style>\n\n\n<script>\n    function win_open(url, name, option)\n    {\n        var popup = window.open(url, name, option);\n        popup.focus();\n    }\n</script>\n';\n\n\n// 전화번호 목록을 배열 형태로도 저장\n$ph